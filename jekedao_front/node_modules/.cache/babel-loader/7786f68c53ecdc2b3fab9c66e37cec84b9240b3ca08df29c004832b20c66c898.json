{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ProblemDetail',\n  props: {\n    problemId: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      title: '',\n      description: ''\n    };\n  },\n  created() {\n    // 获取题目信息\n    this.fetchProblemInfo(this.problemId);\n  },\n  methods: {\n    fetchProblemInfo(problemId) {\n      // 使用 this.$axios 发送请求\n      this.$axios.get(`/api/problems/${problemId}`).then(response => {\n        const data = response.data;\n        this.title = data.title;\n        this.description = data.description;\n      }).catch(error => {\n        console.error('获取题目信息失败:', error);\n        this.title = `题目${problemId}`;\n        this.description = `这是题目${problemId}的描述。`;\n      });\n    },\n    deleteProblem() {\n      // 使用 this.$axios 发送请求\n      this.$axios.delete(`/api/problems/${this.problemId}`).then(() => {\n        this.$message.success('题目删除成功！');\n        this.$router.push('/square');\n      }).catch(error => {\n        console.error('题目删除失败:', error);\n        this.$message.error('题目删除失败，请重试');\n      });\n    },\n    navigateToSolution() {\n      this.$router.push(`/solution/${this.problemId}`);\n    },\n    navigateToSubmit() {\n      this.$router.push(`/submit/${this.problemId}`);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","problemId","type","Number","required","data","title","description","created","fetchProblemInfo","methods","$axios","get","then","response","catch","error","console","deleteProblem","delete","$message","success","$router","push","navigateToSolution","navigateToSubmit"],"sources":["src/components/ProblemDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"problem\">\r\n    <div class=\"problem-header\">\r\n      <h1>题目详情</h1>\r\n      <div class=\"problem-actions\">\r\n        <el-button type=\"danger\" @click=\"deleteProblem\">删除</el-button>\r\n        <el-button type=