{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'ProblemDetail',\n  data() {\n    return {\n      title: '',\n      description: '',\n      problemId: this.$route.params.problemId\n    };\n  },\n  created() {\n    // 获取题目信息\n    this.fetchProblemInfo(this.problemId);\n  },\n  methods: {\n    fetchProblemInfo(problemId) {\n      // 使用 this.$axios 发送请求\n      this.$axios.get(`/problems/${problemId}`).then(response => {\n        const data = response.data;\n        if (data.code === 200) {\n          this.title = data.data.title;\n          this.description = data.data.description;\n        } else {\n          this.$message.error(data.msg || '获取题目信息失败');\n        }\n      }).catch(error => {\n        console.error('获取题目信息失败:', error);\n        this.title = `题目${problemId}`;\n        this.description = `这是题目${problemId}的描述。`;\n      });\n    },\n    deleteProblem() {\n      // 使用 this.$axios 发送请求\n      this.$axios.delete(`/problems/${this.problemId}`).then(() => {\n        this.$message.success('题目删除成功！');\n        this.$router.push('/square');\n      }).catch(error => {\n        console.error('题目删除失败:', error);\n        this.$message.error('题目删除失败，请重试');\n      });\n    },\n    navigateToSolution() {\n      this.$router.push(`/solution/${this.problemId}`);\n    },\n    navigateToSubmit() {\n      this.$router.push(`/submit/${this.problemId}`);\n    }\n  }\n};","map":{"version":3,"names":["name","data","title","description","problemId","$route","params","created","fetchProblemInfo","methods","$axios","get","then","response","code","$message","error","msg","catch","console","deleteProblem","delete","success","$router","push","navigateToSolution","navigateToSubmit"],"sources":["src/components/ProblemDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"problem\">\r\n    <div class=\"problem-header\">\r\n      <h1>题目详情</h1>\r\n      <div class=\"problem-actions\">\r\n        <el-button type=\"danger\" @click=\"deleteProblem\">删除题目</el-button>\r\n        <el-button type=\"primary\" @click=\"navigateToSolution\">查看题解</el-button>\r\n        <el-button type=\"success\" @click=\"navigateToSubmit\">提交代码</el-button>\r\n      </div>\r\n    </div>\r\n    <h2>{{ title }}</h2>\r\n    <p>{{ description }}</p>\r\n    <!-- 可以添加更多题目信息 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ProblemDetail',\r\n  data() {\r\n    return {\r\n      title: '',\r\n      description: '',\r\n      problemId: this.$route.params.problemId,\r\n    };\r\n  },\r\n  created() {\r\n    // 获取题目信息\r\n    this.fetchProblemInfo(this.problemId);\r\n  },\r\n  methods: {\r\n    fetchProblemInfo(problemId) {\r\n      // 使用 this.$axios 发送请求\r\n      this.$axios.get(`/problems/${problemId}`)\r\n        .then(response => {\r\n          const data = response.data;\r\n          if (data.code === 200) {\r\n            this.title = data.data.title;\r\n            this.description = data.data.description;\r\n          } else {\r\n            this.$message.error(data.msg || '获取题目信息失败');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('获取题目信息失败:', error);\r\n          this.title = `题目${problemId}`;\r\n          this.description = `这是题目${problemId}的描述。`;\r\n        });\r\n    },\r\n    deleteProblem() {\r\n      // 使用 this.$axios 发送请求\r\n      this.$axios.delete(`/problems/${this.problemId}`)\r\n        .then(() => {\r\n          this.$message.success('题目删除成功！');\r\n          this.$router.push('/square');\r\n        })\r\n        .catch(error => {\r\n          console.error('题目删除失败:', error);\r\n          this.$message.error('题目删除失败，请重试');\r\n        });\r\n    },\r\n    navigateToSolution() {\r\n      this.$router.push(`/solution/${this.problemId}`);\r\n    },\r\n    navigateToSubmit() {\r\n      this.$router.push(`/submit/${this.problemId}`);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.problem {\r\n  padding: 20px;\r\n}\r\n\r\n.problem-header {\r\n  disp