{"ast":null,"code":"import axios from '@/axios'; // 使用实例化的 Axios\nimport ProblemDetail from './ProblemDetail.vue';\nexport default {\n  name: 'UserProfile',\n  components: {\n    ProblemDetail\n  },\n  data() {\n    return {\n      username: '',\n      userId: '',\n      backgroundProblemId: 1 // 假设背景题目ID为1\n    };\n  },\n  created() {\n    // 获取路由参数中的用户ID\n    this.userId = this.$route.params.userId;\n    // 获取用户信息\n    this.fetchUserInfo(this.userId);\n  },\n  methods: {\n    fetchUserInfo(userId) {\n      axios.get(`/profile/${userId}`).then(response => {\n        if (response.data.code === 200) {\n          this.username = response.data.data.username;\n        } else {\n          this.$message.error(response.data.msg || '获取用户信息失败');\n        }\n      }).catch(error => {\n        console.error('获取用户信息失败:', error);\n        this.$message.error('获取用户信息失败，请重试');\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","ProblemDetail","name","components","data","username","userId","backgroundProblemId","created","$route","params","fetchUserInfo","methods","get","then","response","code","$message","error","msg","catch","console"],"sources":["src/components/UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-profile\">\r\n    <div class=\"background\">\r\n      <ProblemDetail :problemId=\"backgroundProblemId\" />\r\n    </div>\r\n    <div class=\"content\">\r\n      <h1>个人主页</h1>\r\n      <p>用户名: {{ username }}</p>\r\n      <p>用户ID: {{ userId }}</p>\r\n      <!-- 可以在这里添加更多用户信息 -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/axios'; // 使用实例化的 Axios\r\nimport ProblemDetail from './ProblemDetail.vue';\r\n\r\nexport default {\r\n  name: 'UserProfile',\r\n  components: {\r\n    ProblemDetail\r\n  },\r\n  data() {\r\n    return {\r\n      username: '',\r\n      userId: '',\r\n      backgroundProblemId: 1 // 假设背景题目ID为1\r\n    };\r\n  },\r\n  created() {\r\n    // 获取路由参数中的用户ID\r\n    this.userId = this.$route.params.userId;\r\n    // 获取用户信息\r\n    this.fetchUserInfo(this.userId);\r\n  },\r\n  methods: {\r\n    fetchUserInfo(userId) {\r\n      axios.get(`/profile/${userId}`)\r\n        .then(response => {\r\n          if (response.data.code === 200) {\r\n            this.username = response.data.data.username;\r\n          } else {\r\n            this.$message.error(response.data.msg || '获取用户信息失败');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('获取用户信息失败:', error);\r\n          this.$message.error('获取用户信息失败，请重试');\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile {\r\n  position: relative;\r\n  padding: 20px;\r\n}\r\n\r\n.background {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: -1;\r\n  opacity: 0.3; /* 设置背景透明度 */\r\n}\r\n\r\n.content {\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n</style>"],"mappings":"AAeA,OAAAA,KAAA;AACA,OAAAC,aAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,QAAA;MACAC,MAAA;MACAC,mBAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,KAAAF,MAAA,QAAAG,MAAA,CAAAC,MAAA,CAAAJ,MAAA;IACA;IACA,KAAAK,aAAA,MAAAL,MAAA;EACA;EACAM,OAAA;IACAD,cAAAL,MAAA;MACAN,KAAA,CAAAa,GAAA,aAAAP,MAAA,IACAQ,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAX,IAAA,CAAAY,IAAA;UACA,KAAAX,QAAA,GAAAU,QAAA,CAAAX,IAAA,CAAAA,IAAA,CAAAC,QAAA;QACA;UACA,KAAAY,QAAA,CAAAC,KAAA,CAAAH,QAAA,CAAAX,IAAA,CAAAe,GAAA;QACA;MACA,GACAC,KAAA,CAAAF,KAAA;QACAG,OAAA,CAAAH,KAAA,cAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}