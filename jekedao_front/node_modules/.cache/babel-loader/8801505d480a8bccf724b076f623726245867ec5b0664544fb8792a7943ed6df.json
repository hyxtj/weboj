{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from '@/axios'; // 使用配置好的 Axios 实例\nimport ProblemDetail from './ProblemDetail.vue';\nexport default {\n  name: 'SquarePage',\n  components: {\n    ProblemDetail\n  },\n  data() {\n    return {\n      isLoggedIn: false,\n      username: '',\n      problemId: '',\n      backgroundProblemId: 1,\n      // 假设背景题目ID为1\n      problems: []\n    };\n  },\n  created() {\n    this.fetchProblems();\n    this.checkLoginStatus();\n  },\n  methods: {\n    fetchProblems() {\n      axios.get('/problems').then(response => {\n        if (response.data.code === 200) {\n          this.problems = response.data.data;\n        } else {\n          this.$message.error(response.data.msg || '获取题目列表失败');\n        }\n      }).catch(error => {\n        console.error('获取题目列表失败:', error);\n        this.$message.error('获取题目信息失败，请重试');\n      });\n    },\n    checkLoginStatus() {\n      const token = localStorage.getItem('access_token');\n      if (token) {\n        this.isLoggedIn = true;\n        // 获取当前用户信息\n        axios.get('/profile/me').then(response => {\n          if (response.data.code === 200) {\n            this.username = response.data.data.username;\n          } else {\n            this.isLoggedIn = false;\n            this.username = '';\n          }\n        }).catch(error => {\n          console.error('获取用户信息失败:', error);\n          this.isLoggedIn = false;\n          this.username = '';\n        });\n      }\n    },\n    logout() {\n      axios.post('/logout').then(response => {\n        if (response.data.code === 200) {\n          localStorage.removeItem('access_token');\n          this.isLoggedIn = false;\n          this.username = '';\n          this.$router.push('/');\n          this.$message.success('退出登录成功');\n        }\n      }).catch(error => {\n        console.error('退出登录失败:', error);\n        this.$message.error('退出登录失败，请重试');\n      });\n    },\n    searchProblem() {\n      const id = parseInt(this.problemId);\n      if (id && this.problems.some(p => p.id === id)) {\n        this.$router.push(`/problem/${id}`);\n      } else {\n        this.$message.error('请输入有效的题目序号');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","ProblemDetail","name","components","data","isLoggedIn","username","problemId","backgroundProblemId","problems","created","fetchProblems","checkLoginStatus","methods","get","then","response","code","$message","error","msg","catch","console","token","localStorage","getItem","logout","post","removeItem","$router","push","success","searchProblem","id","parseInt","some","p"],"sources":["src/components/Square.vue"],"sourcesContent":["<!-- src/components/Square.vue -->\r\n<template>\r\n  <div class=\"square-container\">\r\n    <!-- 其他模板代码 -->\r\n\r\n    <div class=\"problems\">\r\n      <h3>题目列表</h3>\r\n      <ul>\r\n        <li v-for=\"problem in problems\" :key=\"problem.id\">\r\n          <router-link :to=\"`/problem/${problem.id}`\">{{ problem.title }}</router-link>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <!-- 其他模板代码 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/axios'; // 使用配置好的 Axios 实例\r\nimport ProblemDetail from './ProblemDetail.vue';\r\n\r\nexport default {\r\n  name: 'SquarePage',\r\n  components: {\r\n    ProblemDetail\r\n  },\r\n  data() {\r\n    return {\r\n      isLoggedIn: false,\r\n      username: '',\r\n      problemId: '',\r\n      backgroundProblemId: 1, // 假设背景题目ID为1\r\n      problems: []\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchProblems();\r\n    this.checkLoginStatus();\r\n  },\r\n  methods: {\r\n    fetchProblems() {\r\n      axios.get('/problems')\r\n        .then(response => {\r\n          if (response.data.code === 200) {\r\n            this.problems = response.data.data;\r\n          } else {\r\n            this.$message.error(response.data.msg || '获取题目列表失败');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('获取题目列表失败:', error);\r\n          this.$message.error('获取题目信息失败，请重试');\r\n        });\r\n    },\r\n    checkLoginStatus() {\r\n      const token = localStorage.getItem('access_token');\r\n      if (token) {\r\n        this.isLoggedIn = true;\r\n        // 获取当前用户信息\r\n        axios.get('/profile/me')\r\n          .then(response => {\r\n            if (response.data.code === 200) {\r\n              this.username = response.data.data.username;\r\n            } else {\r\n              this.isLoggedIn = false;\r\n              this.username = '';\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取用户信息失败:', error);\r\n            this.isLoggedIn = false;\r\n            this.username = '';\r\n          });\r\n      }\r\n    },\r\n    logout() {\r\n      axios.post('/logout')\r\n        .then(response => {\r\n          if (response.data.code === 200) {\r\n            localStorage.removeItem('access_token');\r\n            this.isLoggedIn = false;\r\n            this.username = '';\r\n            this.$router.push('/');\r\n            this.$message.success('退出登录成功');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('退出登录失败:', error);\r\n          this.$message.error('退出登录失败，请重试');\r\n        });\r\n    },\r\n    searchProblem() {\r\n      const id = parseInt(this.problemId);\r\n      if (id && this.problems.some(p => p.id === id)) {\r\n        this.$router.push(`/problem/${id}`);\r\n      } else {\r\n        this.$message.error('请输入有效的题目序号');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 保持现有样式不变 */\r\n</style>"],"mappings":";AAmBA,OAAAA,KAAA;AACA,OAAAC,aAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,UAAA;MACAC,QAAA;MACAC,SAAA;MACAC,mBAAA;MAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,aAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACAF,cAAA;MACAX,KAAA,CAAAc,GAAA,cACAC,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAZ,IAAA,CAAAa,IAAA;UACA,KAAAR,QAAA,GAAAO,QAAA,CAAAZ,IAAA,CAAAA,IAA