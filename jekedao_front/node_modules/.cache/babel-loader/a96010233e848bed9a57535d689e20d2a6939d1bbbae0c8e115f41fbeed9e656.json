  },\r\n  methods: {\r\n    register() {\r\n      // 注册逻辑\r\n      alert('注册功能尚未实现');\r\n    },\r\n    login() {\r\n      // 登录逻辑\r\n      this.isLoggedIn = true;\r\n      this.username = '用户'; // 假设用户名为“用户”\r\n    },\r\n    logout() {\r\n      // 退出登录逻辑\r\n      this.isLoggedIn = false;\r\n      this.username = '';\r\n    },\r\n    searchProblem() {\r\n      if (this.problemId) {\r\n        this.$router.push(`/problem/${this.problemId}`);\r\n      } else {\r\n        alert('请输入题目序号');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.square-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 20px;\r\n}\r\n\r\n.left-sidebar, .right-sidebar {\r\n  width: 20%;\r\n  background-color: #f5f5f5;\r\n  padding: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.main-content {\r\n  width: 55%;\r\n  padding: 10px;\r\n}\r\n\r\n.announcement {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.links ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.links li {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.links a {\r\n  color: #007bff;\r\n  text-decoration: none;\r\n}\r\n\r\n.links a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.search {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.search input {\r\n  width: calc(100% - 60px);\r\n  padding: 5px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.search button {\r\n  padding: 5px 10px;\r\n}\r\n\r\n.ranking iframe {\r\n  border: none;\r\n}\r\n</style>"],"mappings":";AAgCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,QAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,SAAA;MACA;MACAC,KAAA;IACA;IACAC,MAAA;MACA;MACA,KAAAN,UAAA;MACA,KAAAC,QAAA;IACA;IACAM,OAAA;MACA;MACA,KAAAP,UAAA;MACA,KAAAC,QAAA;IACA;IACAO,cAAA;MACA,SAAAN,SAAA;QACA,KAAAO,OAAA,CAAAC,IAAA,kBAAAR,SAAA;MACA;QACAG,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}