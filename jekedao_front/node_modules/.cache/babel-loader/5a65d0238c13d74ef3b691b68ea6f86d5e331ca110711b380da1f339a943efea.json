{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from '@/axios';\nexport default {\n  name: 'CreateDiscussion',\n  data() {\n    return {\n      form: {\n        title: '',\n        content: '',\n        problem_id: null\n      }\n    };\n  },\n  methods: {\n    submitForm() {\n      if (!this.form.title || !this.form.content || !this.form.problem_id) {\n        this.$message.error('标题、内容和题目ID为必填项');\n        return;\n      }\n      axios.post('/discussions', this.form).then(response => {\n        if (response.data.code === 201) {\n          this.$message.success('讨论发布成功');\n          this.$router.push('/discussion');\n        } else {\n          this.$message.error(response.data.msg || '讨论发布失败');\n        }\n      }).catch(error => {\n        console.error('讨论发布失败:', error);\n        this.$message.error('讨论发布失败，请重试');\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","title","content","problem_id","methods","submitForm","$message","error","post","then","response","code","success","$router","push","msg","catch","console"],"sources":["src/components/CreateDiscussion.vue"],"sourcesContent":["<template>\r\n  <div class=\"create-discussion\">\r\n    <h1>发布新讨论</h1>\r\n    <el-form ref=\"form\" :model=\"form\" label-width=\"100px\">\r\n      <el-form-item label=\"讨论主题\">\r\n        <el-input v-model=\"form.title\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"讨论内容\">\r\n        <el-input type=\"textarea\" v-model=\"form.content\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"题目ID\">\r\n        <el-input v-model.number=\"form.problem_id\" type=\"number\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm\">提交</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/axios';\r\n\r\nexport default {\r\n  name: 'CreateDiscussion',\r\n  data() {\r\n    return {\r\n      form: {\r\n        title: '',\r\n        content: '',\r\n        problem_id: null\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      if (!this.form.title || !this.form.content || !this.form.problem_id) {\r\n        this.$message.error('标题、内容和题目ID为必填项');\r\n        return;\r\n      }\r\n\r\n      axios.post('/discussions', this.form)\r\n        .then(response => {\r\n          if (response.data.code === 201) {\r\n            this.$message.success('讨论发布成功');\r\n            this.$router.push('/discussion');\r\n          } else {\r\n            this.$message.error(response.data.msg || '讨论发布失败');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('讨论发布失败:', error);\r\n          this.$message.error('讨论发布失败，请重试');\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.create-discussion {\r\n  padding: 20px;\r\n}\r\n\r\nh1 {\r\n  color: #333;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":";AAqBA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAC,OAAA;QACAC,UAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,UAAAL,IAAA,CAAAC,KAAA,UAAAD,IAAA,CAAAE,OAAA,UAAAF,IAAA,CAAAG,UAAA;QACA,KAAAG,QAAA,CAAAC,KAAA;QACA;MACA;MAEAV,KAAA,CAAAW,IAAA,sBAAAR,IAAA,EACAS,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAX,IAAA,CAAAY,IAAA;UACA,KAAAL,QAAA,CAAAM,OAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;QACA;UACA,KAAAR,QAAA,CAAAC,KAAA,CAAAG,QAAA,CAAAX,IAAA,CAAAgB,GAAA;QACA;MACA,GACAC,KAAA,CAAAT,KAAA;QACAU,OAAA,CAAAV,KAAA,YAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}